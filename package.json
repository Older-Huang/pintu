{
  "name": "jigsaw-vt",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.umd.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "author": "Older-Huang",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "npm run clean && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "tsc-build": "npm run clean && vue-tsc && vite build"
  },
  "dependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Older-Huang/pintu/tree/master"
  },
  "bugs": {
    "url": "https://github.com/Older-Huang/pintu/issues"
  },
  "description": "vue3智能拼图小游戏",
  "keywords": [
    "vue"
  ]
}
